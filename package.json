{
  "name": "croud-dj",
  "version": "0.3.0",
  "description": "Let your guests vote on what plays next",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "run-s clean:reports clean:site",
    "clean:reports": "del-cli ./reports ./coverage",
    "clean:site": "del-cli ./site",
    "prebuild": "npm run clean:site",
    "pretest:server": "npm run build",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "server:prod": "node ./server/main.js",
    "start": "run-s build server:prod",
    "test": "run-s test:client test:server",
    "test:client": "cross-env NODE_ENV=test karma start --single-run",
    "test:server": "ava ./server/main.test.js --verbose --tap | tap-xunit > ./reports/server.xml",
    "test:watch": "cross-env NODE_ENV=test karma start --reporters dots"
  },
  "author": {
    "name": "Tamer Wahba",
    "email": "tamwahba@hotmail.com"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "bunyan": "^1.8.10",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "firebase": "^4.1.3",
    "helmet": "^3.6.1",
    "immutable": "^3.8.1",
    "load-script": "^1.0.0",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-bem-helper": "^1.4.1",
    "react-dom": "^15.6.1",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "ava": "^0.19.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "conventional-changelog-cli": "^1.3.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "del-cli": "^1.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.20.2",
    "expect-enzyme": "^1.0.0",
    "expect-immutable": "0.0.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "mocha": "^3.4.2",
    "npm-run-all": "^4.0.2",
    "postcss": "^6.0.2",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.3.4",
    "style-loader": "^0.18.2",
    "supertest": "^3.0.0",
    "svg-sprite-loader": "^3.0.5",
    "tap-xunit": "^1.7.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-md5-hash": "0.0.5"
  }
}
